    "babel-preset-es2015": "^6.24.1",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^3.19.0",
    "eslint-plugin-import": "^1.16.0",
    "html-webpack-plugin": "^2.28.0",
    "mocha": "^2.5.3",
    "node-sass": "^3.13.1",
    "postcss-loader": "^0.9.1",
    "sass-loader": "^4.1.1",
